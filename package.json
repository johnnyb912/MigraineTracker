{
  "name": "depalma-2.x-seed",
  "version": "1.0.0",
  "private": true,
  "description": "DePalma Studios Angular 2.x Seed Project",
  "scripts": {
    "build:dev:mock": "npm run clean:dist && cross-env NODE_ENV=dev webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:dev:wrap": "npm run clean:dist && cross-env NODE_ENV=dev webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:dev:live": "npm run clean:dist && cross-env NODE_ENV=dev webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && cross-env NODE_ENV=prod webpack --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --json > stats.json && npm run clean:post",
    "clean": "rimraf doc .dist .tmp node_modules && npm cache clean",
    "clean:dist": "rimraf doc .dist .tmp",
    "clean:post": "rimraf .tmp",
    "clean-install": "npm run clean && npm install",
    "outdated": "npm outdated",
    "update-packages": "npm update",
    "lint": "tslint --force \"src/app/**/*.ts\" --exclude \"src/app/app.translations.ts\"",
    "stylelint": "stylelint \"src/app/**/*.scss\" --syntax scss ï»¿|| echo 0",
    "dev:mock": "npm run clean:dist && cross-env NODE_ENV=dev webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --open",
    "dev:wrap": "npm run clean:dist && cross-env NODE_ENV=dev webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --open",
    "dev:live": "npm run clean:dist && cross-env NODE_ENV=dev webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --open",
    "dev:prod": "npm run clean:dist && cross-env NODE_ENV=prod webpack-dev-server --config config/webpack.prod.js --progress --profile --watch --open",
    "test": "rimraf coverage && karma start",
    "posttest": "remap-istanbul -i coverage/json/coverage.json -o coverage/html -t html",
    "test-watch": "karma start --no-single-run --auto-watch",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/DePalma_Studios/angular-2.x.git"
  },
  "author": "Ryan Day",
  "license": "ISC",
  "homepage": "https://bitbucket.org/DePalma_Studios/angular-2.x#readme",
